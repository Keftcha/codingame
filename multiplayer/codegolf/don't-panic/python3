dic={}
_,w,_,_,exp,_,_,nb=[int(i) for i in input().split()]
for i in range(nb):
 ef,ep=[int(j) for j in input().split()]
 dic[ef]=ep
while True:
 cf,cp,dire=input().split()
 cf,cp=int(cf),int(cp)
 try:va=dic[cf]
 except:va=exp
 print("BLOCK"if cp<va and dire=="LEFT" or cp>va and dire=="RIGHT" else "WAIT")
